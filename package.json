{
  "name": "inelegis",
  "version": "0.3.7",
  "description": "Sistema de Consulta de Inelegibilidade Eleitoral",
  "type": "module",
  "main": "index.html",
  "scripts": {
    "build": "npm run supabase:config && node scripts/build.js",
    "lint": "eslint src public/assets/js --ext .js",
    "lint:fix": "eslint src public/assets/js --ext .js --fix",
    "lint:theme": "node .agent/hub/capabilities/design/frontend-design/scripts/theme-linter.js",
    "lint:theme:fix": "node .agent/hub/capabilities/design/frontend-design/scripts/theme-linter.js --fix",
    "lint:theme:strict": "node .agent/hub/capabilities/design/frontend-design/scripts/theme-linter.js --strict",
    "validate:theme": "npm run lint:theme",
    "test": "node scripts/test.js",
    "test:coverage": "node scripts/test.js --coverage",
    "test:unit": "node tests/formatters.test.js && node tests/exceptions.test.js",
    "test:theme": "node tests/theme-manager.test.js",
    "test:components": "node tests/components.test.js",
    "test:all": "npm run test:unit && npm run test",
    "test:standards": "node .agent/hub/system/validators/validator-web-standards.js public",
    "sync:js": "node scripts/sync-js.js",
    "supabase:config": "node scripts/build-supabase-config.js",
    "serve": "npm run sync:js && npm run supabase:config && node scripts/serve.js",
    "dev": "npm run check && npm run serve",
    "check": "npm run lint && npm run test && node scripts/build.js --dry-run",
    "audit:docs": "node .agent/hub/capabilities/management/doc-writing/scripts/doc-auditor.js",
    "doc:check": "npm run audit:docs",
    "generate-token": "node scripts/generate-admin-token.js",
    "start": "npm run serve",
    "prebuild": "npm run check",
    "clean": "rimraf dist dist-optimized deploy",
    "clean:all": "npm run clean && rimraf node_modules package-lock.json",
    "reinstall": "npm run clean:all && npm install",
    "update-deps": "npm update && npm audit fix",
    "size": "node -e \"const fs=require('fs'); const path=require('path'); const getSize=d=>fs.readdirSync(d).reduce((s,f)=>{const p=path.join(d,f); return s+(fs.statSync(p).isDirectory()?getSize(p):fs.statSync(p).size)},0); console.log('Tamanho do projeto:', Math.round(getSize('.')/1024)+'KB');\"",
    "verify": "node .agent/hub/system/validators/validator-integrity.js",
    "context": "node .agent/hub/system/generators/build-context.js",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "prepare": "husky"
  },
  "keywords": [
    "inelegibilidade",
    "sistema-juridico"
  ],
  "author": "Inelegis",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.93.3"
  },
  "devDependencies": {
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "html-validate": "^10.6.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.0",
    "puppeteer": "^24.35.0"
  },
  "engines": {
    "node": "22.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rkvasne/inelegis.git"
  },
  "bugs": {
    "url": "https://github.com/rkvasne/inelegis/issues"
  },
  "homepage": "https://github.com/rkvasne/inelegis#readme"
}